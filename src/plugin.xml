<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
    <extension point="com.zutubi.pulse.core.commands">
        <command name="xbuild" class="com.zutubi.pulse.core.commands.xbuild.XBuildCommandConfiguration">
            <resource name="xbuild" optional="true"/>
        </command>
    </extension>
    <extension point="com.zutubi.pulse.core.postprocessors">
        <post-processor name="xbuild.pp" display-name="xbuild output processor"
                        default-processor="true" class="com.zutubi.pulse.core.commands.xbuild.XBuildPostProcessorConfiguration"/>
    </extension>
</plugin>
